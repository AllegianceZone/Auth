				</behavior>

        <behavior name="Allegiance.CommunitySecuritySystem.Server.StatsBehavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>

			</serviceBehaviors>
		</behaviors>

